{
  "manifest_version": 2,
  "name": "Container proxy (entropy ver.)",
  "version": "0.1.22",
  "description": "__MSG_General_extensionDescription__",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "{866ad75f-1c41-4918-a9de-2fda74182230}",
      "strict_min_version": "91.1.0"
    }
  },
  "browser_action": {
  },
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true,
    "browser_style": false
  },

  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "contextualIdentities",
    "cookies",
    "proxy"
  ],

  "background": {
    "scripts": [
      "background/index.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  }
}
